changeNames <- function(number){
name = NULL
if(number == 1)
name = "walking"
else if(number == 2)
name = "walkingUpstairs"
else if(number == 3)
name = "walkingDownstairs"
else if(number == 4)
name = "sitting"
else if(number == 5)
name = "standing"
else if(number == 6)
name = "laying"
name
}
